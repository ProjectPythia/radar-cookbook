 Zoom in by setting the xlim/ylim\nplt.xlim(-99, -96)\nplt.ylim(33.5, 36.5)\nplt.show()","key":"PAKRVOUeUj"},{"type":"output","id":"Eha4Qn4pp3XOQHtGA-_C9","data":[{"output_type":"display_data","metadata":{},"data":{"text/plain":{"content":"<Figure size 1200x400 with 4 Axes>","content_type":"text/plain"},"image/png":{"content_type":"image/png","hash":"06b8e4f47642520f5de5c131d628be8e","path":"/radar-cookbook/build/06b8e4f47642520f5de5c131d628be8e.png"}}}],"key":"CRNqyMfjY4"}],"key":"qSiKf5l6jQ"},{"type":"block","kind":"notebook-content","children":[{"type":"heading","depth":3,"position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Add Counties","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"fgBJN3Naea"}],"identifier":"add-counties","label":"Add Counties","html_id":"add-counties","implicit":true,"key":"LoCsjWzEMU"},{"type":"paragraph","position":{"start":{"line":2,"column":1},"end":{"line":2,"column":1}},"children":[{"type":"text","value":"We can add counties onto our map by using the ","position":{"start":{"line":2,"column":1},"end":{"line":2,"column":1}},"key":"PQhlO83WVG"},{"type":"inlineCode","value":"USCOUNTIES","position":{"start":{"line":2,"column":1},"end":{"line":2,"column":1}},"key":"dhMIUGGGNr"},{"type":"text","value":" module from ","position":{"start":{"line":2,"column":1},"end":{"line":2,"column":1}},"key":"CluZjKGK0n"},{"type":"inlineCode","value":"metpy.plots","position":{"start":{"line":2,"column":1},"end":{"line":2,"column":1}},"key":"AnxQ1JSKo4"}],"key":"pNr1XxmBkB"}],"key":"rHU9oT7PFM"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"# Create our figure\nfig = plt.figure(figsize=[12, 4])\n\n# Setup our first axis with reflectivity\nax1 = plt.subplot(121, projection=ccrs.PlateCarree())\ndisplay = pyart.graph.RadarMapDisplay(radar)\ndisplay.plot_ppi_map('reflectivity',\n                     sweep=3,\n         