 Zoom in by setting the xlim/ylim\nplt.xlim(-99, -96)\nplt.ylim(33.5, 36.5)\nplt.show()","key":"pRuBWODExf"},{"type":"output","id":"eXt7fA9VNKG7xGe2Omfsl","data":[{"output_type":"display_data","metadata":{},"data":{"text/plain":{"content":"<Figure size 1200x400 with 4 Axes>","content_type":"text/plain"},"image/png":{"content_type":"image/png","hash":"06b8e4f47642520f5de5c131d628be8e","path":"/radar-cookbook/build/06b8e4f47642520f5de5c131d628be8e.png"}}}],"key":"SK0ZVshT4j"}],"key":"bIDPN81EI9"},{"type":"block","kind":"notebook-content","children":[{"type":"heading","depth":3,"position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Add Counties","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"fAV5vrNCTZ"}],"identifier":"add-counties","label":"Add Counties","html_id":"add-counties","implicit":true,"key":"fuU07fw0vj"},{"type":"paragraph","position":{"start":{"line":2,"column":1},"end":{"line":2,"column":1}},"children":[{"type":"text","value":"We can add counties onto our map by using the ","position":{"start":{"line":2,"column":1},"end":{"line":2,"column":1}},"key":"oE0LKzb0nd"},{"type":"inlineCode","value":"USCOUNTIES","position":{"start":{"line":2,"column":1},"end":{"line":2,"column":1}},"key":"XUnvXECLQS"},{"type":"text","value":" module from ","position":{"start":{"line":2,"column":1},"end":{"line":2,"column":1}},"key":"U7VObeFx6Z"},{"type":"inlineCode","value":"metpy.plots","position":{"start":{"line":2,"column":1},"end":{"line":2,"column":1}},"key":"ynD9EVxP5H"}],"key":"n7l0fN7kuw"}],"key":"ssWPPFEeGv"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"# Create our figure\nfig = plt.figure(figsize=[12, 4])\n\n# Setup our first axis with reflectivity\nax1 = plt.subplot(121, projection=ccrs.PlateCarree())\ndisplay = pyart.graph.RadarMapDisplay(radar)\ndisplay.plot_ppi_map('reflectivity',\n                     sweep=3,\n         