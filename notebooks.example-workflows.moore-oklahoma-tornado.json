n=ccrs.PlateCarree(),\n                     ax=ax2,)\n\n# Zoom in by setting the xlim/ylim\nplt.xlim(-99, -96)\nplt.ylim(33.5, 36.5)\n\n# Add counties\nax2.add_feature(USCOUNTIES,\n                linewidth=0.5)\n\nplt.show()","key":"yXvYckTmtk"},{"type":"output","id":"QH-VR5hsnE5I6CdUueW1a","data":[{"output_type":"stream","name":"stderr","text":"Downloading file 'us_counties_20m.cpg' from 'https://github.com/Unidata/MetPy/raw/v1.7.0/staticdata/us_counties_20m.cpg' to '/home/runner/.cache/metpy/v1.7.0'.\n"},{"output_type":"stream","name":"stderr","text":"Downloading file 'us_counties_20m.dbf' from 'https://github.com/Unidata/MetPy/raw/v1.7.0/staticdata/us_counties_20m.dbf' to '/home/runner/.cache/metpy/v1.7.0'.\n"},{"output_type":"stream","name":"stderr","text":"Downloading file 'us_counties_20m.prj' from 'https://github.com/Unidata/MetPy/raw/v1.7.0/staticdata/us_counties_20m.prj' to '/home/runner/.cache/metpy/v1.7.0'.\n"},{"output_type":"stream","name":"stderr","text":"Downloading file 'us_counties_20m.shx' from 'https://github.com/Unidata/MetPy/raw/v1.7.0/staticdata/us_counties_20m.shx' to '/home/runner/.cache/metpy/v1.7.0'.\n"},{"output_type":"stream","name":"stderr","text":"Downloading file 'us_counties_20m.shp' from 'https://github.com/Unidata/MetPy/raw/v1.7.0/staticdata/us_counties_20m.shp' to '/home/runner/.cache/metpy/v1.7.0'.\n"},{"output_type":"display_data","metadata":{},"data":{"text/plain":{"content":"<Figure size 1200x400 with 4 Axes>","content_type":"text/plain"},"image/png":{"content_type":"image/png","hash":"207a881e232fdca5858f905165a0f1d5","path":"/radar-cookbook/build/207a881e232fdca5858f905165a0f1d5.png"}}}],"key":"nvB1L0rzOZ"}],"key":"NHTY6d2nGl"},{"type":"block","kind":"notebook-content","children":[{"type":"heading","depth":3,"position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Zoom in even more","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"R4z9euepKZ"}],"identifier":"zoom-in-even-more","label":"Zoom in even more","html_id":"zoom-in-even-more","implicit":true,"key":"Y6g4UQGrY0"},{"type":"paragraph","position":{"start":{"line":2,"column":1},"end":{"line":2,"column":1}},"children":[{"type":"text","value":"Let’s zoom in even more to our main feature - it looks like there is velocity couplet (where high positive and negative values of velcocity are close to one another, indicating rotation), near the center of our map.","position":{"start":{"line":2,"column":1},"end":{"line":2,"column":1}},"key":"w5qw7FibPc"}],"key":"JwkXBdlH0E"}],"key":"yZ9agT6u7o"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"# Create our figure\nfig = plt.figure(figsize=[12, 4])\n\n# Setup our first axis with reflectivity\nax1 = plt.subplot(121, projection=ccrs.PlateCarree())\ndisplay = pyart.graph.RadarMapDisplay(radar)\ndisplay.plot_ppi_map('reflectivity',\n                     sweep=3,\n                     vmin=-20,\n                     vmax=60,\n                     ax=ax1,)\n\n# Zoom in by setting the xlim/ylim\nplt.xlim(-98, -97)\nplt.ylim(35, 36)\n\n# Add counties\nax1.add_feature(USCOUNTIES,\n                linewidth=0.5)\n\n# Setup our second axis for velocity\nax2 = plt.subplot(122, projection=ccrs.PlateCarree())\ndisplay.plot_ppi_map('velocity',\n                     sweep=3,\n                     vmin=-40,\n                     vmax=40,\n                     projection=ccrs.PlateCarree(),\n                     ax=ax2,)\n\n# Zoom in by setting the xlim/ylim\nplt.xlim(-98, -97)\nplt.ylim(35, 36)\n\n# Add counties\nax2.add_feature(USCOUNTIES,\n                linewidth=0.5)\n\nplt.show()","key":"Wqk93N7Ilh"},{"type":"output","id":"6r1USfp37Z4qLvdFt8iVv","data":[{"output_type":"display_data","metadata":{},"data":{"text/plain":{"content":"<Figure size 1200x400 with 4 Axes>","content_type":"text/plain"},"image/png":{"content_type":"image/png","hash":"5d52ff4ac98ccea4230664e02be63b49","path":"/radar-cookbook/build/5d52ff4ac98ccea4230664e02be63b49.png"}}}],"key":"DfevVv8Kzg"}],"key":"ec7L8cus84"},{"type":"block","kind":"notebook-content","children":[{"type":"heading","depth":3,"position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Customize our Labels and Add Finer Grid Labels","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"CECIY12tv7"}],"identifier":"customize-our-labels-and-add-finer-grid-labels","label":"Customize our Labels and Add Finer Grid Labels","html_id":"customize-our-labels-and-add-finer-grid-labels","implicit":true,"key":"lX90SyvlMY"},{"type":"paragraph","position":{"start":{"line":2,"column":1},"end":{"line":2,"column":1}},"children":[{"type":"text","value":"You’ll notice, by default, our colorbar label for the velocity field on the right extends across our entire figure, and the latitude/longitude labels on our axes are now gone. Let’s fix that!","position":{"start":{"line":2,"column":1},"end":{"line":2,"column":1}},"key":"KXfvWRn7ec"}],"key":"VuyoosQ08k"}],"key":"jKnYrINUvV"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"# Create our figure\nfig = plt.figure(figsize=[12, 4])\n\n# Setup our first axis with reflectivity\nax1 = plt.subplot(121, projection=ccrs.PlateCarree())\ndisplay = pyart.graph.RadarMapDisplay(radar)\nref_map = display.plot_ppi_map('reflectivity',\n                               sweep=3,\n                               vmin=-20,\n                               vmax=60,\n                               ax=ax1,\n                               colorbar_label='Equivalent Relectivity ($Z_{e}$) (dBZ)')\n\n\n# Zoom in by setting the xlim/ylim\nplt.xlim(-97.7, -97.1)\nplt.ylim(35.2, 35.8)\n\n# Add gridlines\ngl = ax1.gridlines(crs=ccrs.PlateCarree(),\n                  draw_labels=True,\n                  linewidth=1, \n                  color='gray', \n                  alpha=0.3,\n                  linestyle='--')\n\n# Make sure labels are only plotted on the left and bottom\ngl.xlabels_top = False\ngl.ylabels_right = False\n\n# Increase the fontsize of our gridline labels\ngl.xlabel_style = {'fontsize':10}\ngl.ylabel_style = {'fontsize':10}\n\n# Add counties\nax1.add_feature(USCOUNTIES,\n                linewidth=0.5)\n\n# Setup our second axis for velocity\nax2 = plt.subplot(122, projection=ccrs.PlateCarree())\nvel_plot = display.plot_ppi_map('velocity',\n                                sweep=3,\n                                vmin=-40,\n                                vmax=40,\n                                projection=ccrs.PlateCarree(),\n                                ax=ax2,\n                                colorbar_label='Radial Velocity (m/s)')\n\n# Zoom in by setting the xlim/ylim\nplt.xlim(-97.7, -97.1)\nplt.ylim(35.2, 35.8)\n\n# Add gridlines\ngl = ax2.gridlines(crs=ccrs.PlateCarree(),\n                   draw_labels=True,\n                   linewidth=1, \n                   color='gray', \n                   alpha=0.3,\n                   linestyle='--')\n\n# Make sure labels are only plotted on the left and bottom\ngl.xlabels_top = False\ngl.ylabels_right = False\n\n# Increase the fontsize of our gridline labels\ngl.xlabel_style = {'fontsize':10}\ngl.ylabel_style = {'fontsize':10}\n\n# Add counties\nax2.add_feature(USCOUNTIES,\n                linewidth=0.5)\nplt.show()","key":"W6d6MJlZ0G"},{"type":"output","id":"Z9_lRaForc8LWWc0ZzbLO","data":[{"output_type":"display_data","metadata":{},"data":{"text/plain":{"content":"<Figure size 1200x400 with 4 Axes>","content_type":"text/plain"},"image/png":{"content_type":"image/png","hash":"3cfd05c027d9f10099a2ea3f2ad88afd","path":"/radar-cookbook/build/3cfd05c027d9f10099a2ea3f2ad88afd.png"}}}],"key":"LAOF6WXEsK"}],"key":"n9SV2CuhU2"},{"type":"block","kind":"notebook-content","children":[{"type":"thematicBreak","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}